package Lista04;

public class Cachorro extends AnimalAB {
	private int quantidadePercorrida;
	private int quantidadeComida;
	private int quantidadeDormir;
	@Override
	public void comer() {
		quantidadeComida += 2;
		System.out.println("O cachorro está comendo. Quantidade de comida ingerida: " + quantidadeComida);
	}

	@Override
	public void moverse() {
		quantidadePercorrida += 2;
		System.out.println("O cachorro está se movendo. Quantidade de distancia percorrida: " + quantidadePercorrida);
	}

	@Override
	public void dormir() {
		quantidadeDormir += 5;
		System.out.println("O cachorro está dormindo. Quantidade de Horas dormida: " + quantidadeDormir);

	}
}
